RewriteEngine On
RewriteRule ^player_api\.php/?$ playlist.php?api=xtream [QSA,L]
RewriteRule ^get\.php/?$ playlist.php?api=xtream_get [QSA,L]
RewriteRule ^xmltv\.php/?$ playlist.php?api=xtream_xmltv [QSA,L]
RewriteRule ^live/([^/]+)/([^/]+)/([A-Za-z0-9_]+)\.(m3u8|ts)/?$ playlist.php?api=xtream_live&username=$1&password=$2&stream_id=$3&output=$4 [QSA,L]
RewriteRule ^raw_([A-Za-z0-9-=-_-!-~--]+)\.m3u8/?$ live.php?id=$1 [NC,L]
RewriteRule ^raw/([A-Za-z0-9-=-_-!-~--]+)\.m3u8/?$ live.php?id=$1 [NC,L]
RewriteRule ^upraw_([A-Za-z0-9-=-_-!-~--]+)\.m3u8/?$ playlist.php?api=raw&id=$1 [NC,L]
RewriteRule ^upraw/([A-Za-z0-9-=-_-!-~--]+)\.m3u8/?$ playlist.php?api=raw&id=$1 [NC,L]
RewriteRule ^details_([A-Za-z0-9-=-_-!-~--]+)/?$ details.php?data=$1 [NC,L]
RewriteRule ^play_([A-Za-z0-9-=-_-!-~--]+)/?$ play.php?data=$1 [NC,L]
RewriteRule ^ts_live_([A-Za-z0-9-=-_-!-~--]+).m3u8/?$ live.php?id=$1 [NC,L]
RewriteRule ^tsjio_playlist.m3u/?$ playlist.php [NC,L]
RewriteRule ^ts_sony_live_([A-Za-z0-9-=-_-!-~--]+).m3u8/?$ s_live.php?id=$1 [NC,L]
